{
  "env": {
    "mongo": true,
    "node": true,
    "es2021": true,
    "jest": true
  },
  "plugins": ["security"],
  "extends": ["airbnb-base", "plugin:security/recommended", "prettier"],
  "rules": {
    "no-underscore-dangle": [
      "error",
      {
        "allow": ["_id", "_doc"]
      }
    ]
  }
}
